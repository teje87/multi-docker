{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "teje87/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "teje87/multi-server",
            "hostmanme": "api",
            "essential": false,
            "memory": 128

        },
        {
            "name": "worker",
            "image": "teje87/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "teje87/multi-nginx",
            "hostmane": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]
}